alias(
  "jobkill",
  fn (bid, cmd, args, raw) {
    if len(args) != 1 {
      return m_error(bid, "invalid number of arguments");
    }

    m_notify(bid, "tasked beacon to kill job");
    return b_jobkill(bid, int(args[0]));
  },
  "kill job, controlled by beacon",
  "Usage:\n\tjobkill [jid]",
  true
);