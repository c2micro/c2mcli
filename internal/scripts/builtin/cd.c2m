alias(
  "cd",
  fn (bid, cmd, args, raw) {
    if len(args) != 1 {
      return m_error(bid, "invalid number of arguments");
    }

    m_notify(bid, "tasked beacon to change working directory");
    return b_cd(bid, args[0]);
  },
  "change working directory of beacon process",
  "Usage:\n\tcd [path]",
  true
);