alias(
  "cat",
  fn (bid, cmd, args, raw) {
    if args.len() != 1 {
      return m_error(bid, "invalid number of arguments");
    }

    m_notify(bid, "tasked beacon to print content of file");
    return b_cat(bid, args[0]);
  },
  "print content of file to console",
  "Usage:\n\tcat [file]",
  true
);